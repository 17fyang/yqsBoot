<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<classPathEntry
		location="E:\maven\repository\mysql\mysql-connector-java\8.0.16\mysql-connector-java-8.0.16.jar" />
	<context id="context1" targetRuntime="MyBatis3">
		<!-- 取消注释 -->
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 设置数据库 -->
		<jdbcConnection
			connectionURL="jdbc:mysql://localhost:3306/yqs?serverTimezone=UTC&amp;nullCatalogMeansCurrent=true&amp;useUnicode=true&amp;characterEncoding=utf8"
			driverClass="com.mysql.cj.jdbc.Driver" password="123456"
			userId="root" >
		</jdbcConnection>

		<!-- targetPackage 和 targetProject：生成的 mapper 文件的包和位置 -->
		<javaModelGenerator
			targetPackage="com.stu.yqs.domain2" targetProject="src\main\java" />
		<sqlMapGenerator targetPackage="com.stu.yqs.mappers2"
			targetProject="src\main\java" />

		<!-- targetPackage 和 targetProject：生成的 interface 文件的包和位置 -->
		<javaClientGenerator
			targetPackage="com.stu.yqs.dao2" targetProject="src\main\java"
			type="XMLMAPPER" />
		<table tableName="yqs_user" domainObjectName="User"
		enableCountByExample="false"
		enableUpdateByExample="false"
		enableDeleteByExample="false"
		enableSelectByExample="false"
		selectByExampleQueryId="false">
		<property name="useActualColumnNames" value=" true" />
		<columnOverride column="idtype" property="IDDDDDD" />
		</table>
		<table tableName="yqs_good"  domainObjectName="Good"
		enableCountByExample="false"
		enableUpdateByExample="false"
		enableDeleteByExample="false"
		enableSelectByExample="false"
		selectByExampleQueryId="false">
			<property name=" useActualColumnNames"  value=" true" />
			
		</table>
	</context>


</generatorConfiguration>